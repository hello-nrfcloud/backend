{
  "name": "@nordicsemiconductor/nrf-guide-backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for nRF guide developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "pre-test": "npx tsx ./e2e-tests/nrfcloud-mock/index.ts mock",
    "post-test": "npx tsx ./e2e-tests/nrfcloud-mock/index.ts cleanup",
    "test": "jest --passWithNoTests",
    "test:e2e": "npx tsx ./feature-runner/run-features.ts | npx tsx ./feature-runner/console-reporter.ts",
    "prepare": "husky install && check-node-version --package",
    "deploy": "cdk deploy",
    "synth": "cdk synth"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/nrf.guide-backend.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/nrf.guide-backend/issues"
  },
  "homepage": "https://github.com/bifravst/nrf.guide-backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "nrf-guide",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "1.5.1",
    "@aws-sdk/client-apigatewaymanagementapi": "3.272.0",
    "@aws-sdk/client-dynamodb": "3.272.0",
    "@aws-sdk/client-ecs": "3.272.0",
    "@aws-sdk/client-eventbridge": "3.272.0",
    "@aws-sdk/client-iot": "^3.283.0",
    "@aws-sdk/client-sqs": "^3.282.0",
    "@aws-sdk/util-dynamodb": "3.272.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.180",
    "@nordicsemiconductor/bdd-markdown": "^3.5.32",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "11.0.3",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.36",
    "@swc/jest": "0.2.24",
    "@types/aws-lambda": "8.10.110",
    "@types/glob": "8.0.1",
    "@types/needle": "^3.2.0",
    "@types/ws": "^8.5.4",
    "@types/yargs": "^17.0.22",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.65.0",
    "cdk": "2.65.0",
    "check-node-version": "4.2.1",
    "constructs": "10.1.256",
    "glob": "8.1.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "mqtt": "^4.3.7",
    "prettier": "2.8.4",
    "tsx": "3.12.3",
    "typescript": "5.0.2",
    "ws": "^8.12.1",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "volta": {
    "node": "18.14.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.279.0",
    "@aws-sdk/client-ssm": "3.272.0",
    "@nordicsemiconductor/cloudformation-helpers": "7.0.7",
    "@nordicsemiconductor/from-env": "1.0.3",
    "@sinclair/typebox": "0.25.23",
    "ajv": "8.12.0",
    "yargs": "17.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
