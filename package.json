{
  "name": "@hello.nrfcloud.com/backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for hello.nrfcloud.com developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:e2e": "npx tsx --no-warnings ./feature-runner/run-features.ts > e2e-test-result.json && cat e2e-test-result.json | npx tsx --no-warnings ./feature-runner/console-reporter.ts --only-failed --with-timestamps",
    "prepare": "husky install && check-node-version --package",
    "deploy": "cdk deploy",
    "synth": "cdk synth"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/backend.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/backend/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "1.12.1",
    "@aws-sdk/client-apigatewaymanagementapi": "3.405.0",
    "@aws-sdk/client-cloudformation": "3.405.0",
    "@aws-sdk/client-cloudwatch-logs": "3.405.0",
    "@aws-sdk/client-dynamodb": "3.405.0",
    "@aws-sdk/client-ecr": "3.405.0",
    "@aws-sdk/client-ecs": "3.406.0",
    "@aws-sdk/client-eventbridge": "3.406.0",
    "@aws-sdk/client-iam": "3.405.0",
    "@aws-sdk/client-iot": "3.405.0",
    "@aws-sdk/client-sqs": "3.405.0",
    "@aws-sdk/client-ssm": "3.405.0",
    "@aws-sdk/client-sts": "3.405.0",
    "@aws-sdk/client-timestream-write": "3.405.0",
    "@aws-sdk/util-dynamodb": "3.405.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.83",
    "@nordicsemiconductor/bdd-markdown": "5.6.12",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.17",
    "@nordicsemiconductor/firmware-ci-device-helpers": "14.0.191",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.82",
    "@swc/jest": "0.2.29",
    "@types/aws-lambda": "8.10.119",
    "@types/chai": "4.3.6",
    "@types/chai-subset": "1.3.3",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.4",
    "@types/lodash-es": "4.17.9",
    "@types/ws": "8.5.5",
    "@types/yazl": "2.4.2",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "aws-cdk": "2.94.0",
    "aws-cdk-lib": "2.94.0",
    "cdk": "2.94.0",
    "chai": "4.3.8",
    "chai-subset": "1.6.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "11.0.0",
    "constructs": "10.2.70",
    "eslint-plugin-unicorn": "48.0.1",
    "glob": "10.3.4",
    "husky": "8.0.3",
    "id128": "1.6.6",
    "jest": "29.6.4",
    "jsonata": "2.0.3",
    "mqtt": "5.0.4",
    "p-retry": "6.0.0",
    "prettier": "3.0.3",
    "semver": "7.5.4",
    "tsmatchers": "4.0.12",
    "tsx": "3.12.8",
    "typescript": "5.2.2",
    "ws": "8.13.0",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-lambda-powertools/metrics": "1.12.1",
    "@hello.nrfcloud.com/proto": "5.5.4",
    "@middy/core": "4.6.1",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@nordicsemiconductor/timestream-helpers": "5.0.0",
    "@sinclair/typebox": "0.31.5",
    "ajv": "8.12.0",
    "lodash-es": "4.17.21",
    "p-limit": "4.0.0",
    "table": "6.8.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "lodash_es"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "$schema": "https://json.schemastore.org/swcrc",
          "jsc": {
            "parser": {
              "syntax": "typescript"
            }
          }
        }
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
