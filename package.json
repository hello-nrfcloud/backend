{
  "name": "@hello.nrfcloud.com/backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for hello.nrfcloud.com developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "npx globstar -- npx tsx --test --test-reporter spec \"!(dist|node_modules)/**/*.spec.ts\"",
    "test:e2e": "npx tsx --no-warnings ./feature-runner/run-features.ts > e2e-test-result.json && cat e2e-test-result.json | npx tsx --no-warnings ./feature-runner/console-reporter.ts --only-failed --with-timestamps",
    "prepare": "husky && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/backend.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/backend/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "3.569.0",
    "@aws-sdk/client-cloudformation": "3.569.0",
    "@aws-sdk/client-cloudwatch-logs": "3.569.0",
    "@aws-sdk/client-dynamodb": "3.569.0",
    "@aws-sdk/client-ecr": "3.569.0",
    "@aws-sdk/client-ecs": "3.569.0",
    "@aws-sdk/client-eventbridge": "3.569.0",
    "@aws-sdk/client-iam": "3.569.0",
    "@aws-sdk/client-iot": "3.569.0",
    "@aws-sdk/client-iot-data-plane": "3.569.0",
    "@aws-sdk/client-lambda": "3.569.0",
    "@aws-sdk/client-ses": "3.569.0",
    "@aws-sdk/client-sqs": "3.569.0",
    "@aws-sdk/client-ssm": "3.569.0",
    "@aws-sdk/client-sts": "3.569.0",
    "@aws-sdk/client-timestream-query": "3.569.0",
    "@aws-sdk/client-timestream-write": "3.569.0",
    "@aws-sdk/util-dynamodb": "3.569.0",
    "@bifravst/aws-cdk-ecr-helpers": "1.2.6",
    "@bifravst/aws-cdk-lambda-helpers": "1.7.8",
    "@bifravst/ci": "2.0.1",
    "@bifravst/eslint-config-typescript": "6.0.25",
    "@bifravst/http-api-mock": "1.4.29",
    "@bifravst/prettier-config": "1.0.0",
    "@commitlint/config-conventional": "19.2.2",
    "@hello.nrfcloud.com/bdd-markdown-steps": "2.4.19",
    "@nordicsemiconductor/bdd-markdown": "8.1.11",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.3",
    "@nordicsemiconductor/firmware-ci-device-helpers": "14.0.201",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.5.5",
    "@types/aws-lambda": "8.10.137",
    "@types/glob": "8.1.0",
    "@types/lodash-es": "4.17.12",
    "@types/semver": "7.5.8",
    "@types/ws": "8.5.10",
    "@types/yazl": "2.4.5",
    "aws-cdk": "2.140.0",
    "aws-cdk-lib": "2.140.0",
    "cbor-x": "1.5.9",
    "cdk": "2.140.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "12.0.0",
    "commitlint": "19.3.0",
    "constructs": "10.3.0",
    "glob": "10.3.12",
    "globstar": "1.0.0",
    "husky": "9.0.11",
    "jsonata": "2.0.4",
    "p-retry": "6.2.0",
    "semver": "7.6.1",
    "tsmatchers": "5.0.2",
    "tsx": "4.9.3",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@bifravst/prettier-config",
  "dependencies": {
    "@aws-lambda-powertools/metrics": "2.1.0",
    "@bifravst/aws-ssm-settings-helpers": "1.1.10",
    "@bifravst/run": "1.2.0",
    "@hello.nrfcloud.com/certificate-helpers": "1.0.0",
    "@hello.nrfcloud.com/lambda-helpers": "1.1.9",
    "@hello.nrfcloud.com/nrfcloud-api-helpers": "3.0.5",
    "@hello.nrfcloud.com/proto": "8.0.0-development.1",
    "@hello.nrfcloud.com/proto-map": "7.1.1",
    "@middy/core": "5.3.2",
    "@nordicsemiconductor/from-env": "3.0.1",
    "@nordicsemiconductor/random-words": "8.0.1",
    "@nordicsemiconductor/timestream-helpers": "6.0.2",
    "@sinclair/typebox": "0.32.29",
    "id128": "1.6.6",
    "lodash-es": "4.17.21",
    "mqtt": "5.5.5",
    "p-limit": "5.0.0",
    "table": "6.8.2",
    "ws": "8.17.0"
  }
}
