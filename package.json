{
  "name": "@hello.nrfcloud.com/backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for hello.nrfcloud.com developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "npx globstar -- npx tsx --test --test-reporter spec \"!(dist|node_modules)/**/*.spec.ts\"",
    "test:e2e": "npx tsx --no-warnings ./feature-runner/run-features.ts > e2e-test-result.json && cat e2e-test-result.json | npx tsx --no-warnings ./feature-runner/console-reporter.ts --only-failed --with-timestamps",
    "prepare": "husky && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/backend.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/backend/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-acm": "3.670.0",
    "@aws-sdk/client-apigatewaymanagementapi": "3.670.0",
    "@aws-sdk/client-cloudformation": "3.674.0",
    "@aws-sdk/client-cloudwatch-logs": "3.674.0",
    "@aws-sdk/client-dynamodb": "3.674.0",
    "@aws-sdk/client-ecr": "3.670.0",
    "@aws-sdk/client-ecs": "3.674.0",
    "@aws-sdk/client-eventbridge": "3.674.0",
    "@aws-sdk/client-iam": "3.670.0",
    "@aws-sdk/client-iot": "3.674.0",
    "@aws-sdk/client-iot-data-plane": "3.670.0",
    "@aws-sdk/client-lambda": "3.670.0",
    "@aws-sdk/client-route-53": "3.670.0",
    "@aws-sdk/client-ses": "3.670.0",
    "@aws-sdk/client-sfn": "3.674.0",
    "@aws-sdk/client-sqs": "3.670.0",
    "@aws-sdk/client-ssm": "3.674.0",
    "@aws-sdk/client-sts": "3.670.0",
    "@aws-sdk/client-timestream-query": "3.674.0",
    "@aws-sdk/client-timestream-write": "3.674.0",
    "@aws-sdk/util-dynamodb": "3.674.0",
    "@bifravst/aws-cdk-ecr-helpers": "1.3.24",
    "@bifravst/aws-cdk-lambda-helpers": "1.10.32",
    "@bifravst/bdd-markdown": "8.2.26",
    "@bifravst/ci": "2.0.2",
    "@bifravst/cloudformation-helpers": "9.1.1",
    "@bifravst/eslint-config-typescript": "6.1.18",
    "@bifravst/http-api-mock": "2.1.39",
    "@bifravst/prettier-config": "1.1.1",
    "@bifravst/random-words": "8.0.2",
    "@commitlint/config-conventional": "19.5.0",
    "@hello.nrfcloud.com/bdd-markdown-steps": "2.8.177",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.36",
    "@types/aws-lambda": "8.10.145",
    "@types/cfn-response": "1.0.8",
    "@types/glob": "8.1.0",
    "@types/jsonwebtoken": "9.0.7",
    "@types/lodash-es": "4.17.12",
    "@types/semver": "7.5.8",
    "@types/ws": "8.5.12",
    "@types/yauzl": "2.10.3",
    "@types/yazl": "2.4.5",
    "aws-cdk": "2.162.1",
    "aws-cdk-lib": "2.162.1",
    "cbor-x": "1.6.0",
    "cdk": "2.162.1",
    "cfn-response": "1.0.1",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "12.1.0",
    "commitlint": "19.5.0",
    "constructs": "10.4.2",
    "glob": "11.0.0",
    "globstar": "1.0.0",
    "husky": "9.1.6",
    "jsonata": "2.0.5",
    "lint-staged": "15.2.10",
    "nock": "14.0.0-beta.15",
    "semver": "7.6.3",
    "tsmatchers": "5.0.2",
    "tsx": "4.19.1",
    "yauzl": "3.1.3",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@bifravst/prettier-config",
  "dependencies": {
    "@aws-lambda-powertools/metrics": "2.9.0",
    "@bifravst/aws-ssm-settings-helpers": "1.2.46",
    "@bifravst/from-env": "3.0.2",
    "@bifravst/run": "1.2.0",
    "@bifravst/timestream-helpers": "6.0.3",
    "@hello.nrfcloud.com/certificate-helpers": "1.0.0",
    "@hello.nrfcloud.com/lambda-helpers": "2.0.41",
    "@hello.nrfcloud.com/nrfcloud-api-helpers": "6.0.32",
    "@hello.nrfcloud.com/proto": "15.3.11",
    "@hello.nrfcloud.com/proto-map": "16.1.14",
    "@middy/core": "5.5.1",
    "@middy/input-output-logger": "5.5.1",
    "@sinclair/typebox": "0.33.17",
    "cbor-x": "1.6.0",
    "id128": "1.6.6",
    "jsonwebtoken": "9.0.2",
    "lodash-es": "4.17.21",
    "mqtt": "5.10.1",
    "p-limit": "6.1.0",
    "p-retry": "6.2.0",
    "semver": "7.6.3",
    "table": "6.8.2",
    "ws": "8.18.0"
  }
}
