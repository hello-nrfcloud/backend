{
  "name": "@nordicsemiconductor/nrf-guide-backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for nRF guide developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "jest --passWithNoTests",
    "prepare": "husky install && check-node-version --package",
    "deploy": "cdk deploy",
    "synth": "cdk synth"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/nrf.guide-backend.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/nrf.guide-backend/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/nrf.guide-backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "nrf-guide",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "1.5.1",
    "@aws-sdk/client-apigatewaymanagementapi": "3.267.0",
    "@aws-sdk/client-dynamodb": "3.267.0",
    "@aws-sdk/client-eventbridge": "3.267.0",
    "@aws-sdk/util-dynamodb": "3.267.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.177",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "11.0.2",
    "@swc/cli": "0.1.61",
    "@swc/core": "1.3.35",
    "@swc/jest": "0.2.24",
    "@types/aws-lambda": "8.10.110",
    "@types/glob": "8.0.1",
    "@types/needle": "^3.2.0",
    "@types/yargs": "^17.0.22",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.64.0",
    "cdk": "2.64.0",
    "check-node-version": "4.2.1",
    "constructs": "10.1.248",
    "glob": "8.1.0",
    "husky": "8.0.3",
    "jest": "29.4.2",
    "prettier": "2.8.4",
    "tsx": "3.12.3",
    "typescript": "4.9.5",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "volta": {
    "node": "18.13.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-sdk/client-iot": "^3.267.0",
    "@aws-sdk/client-ssm": "^3.267.0",
    "@nordicsemiconductor/from-env": "1.0.3",
    "@nordicsemiconductor/timestream-helpers": "4.1.3",
    "@sinclair/typebox": "0.25.21",
    "ajv": "8.12.0",
    "needle": "^3.2.0",
    "yargs": "^17.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
