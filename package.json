{
  "name": "@hello.nrfcloud.com/backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for hello.nrfcloud.com developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "npx globstar -- npx tsx --test --test-reporter spec \"!(dist|node_modules)/**/*.spec.ts\"",
    "test:e2e": "npx tsx --no-warnings ./feature-runner/run-features.ts > e2e-test-result.json && cat e2e-test-result.json | npx tsx --no-warnings ./feature-runner/console-reporter.ts --only-failed --with-timestamps",
    "prepare": "husky install && check-node-version --package",
    "deploy": "cdk deploy",
    "synth": "cdk synth"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/backend.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/backend/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "1.17.0",
    "@aws-sdk/client-apigatewaymanagementapi": "3.490.0",
    "@aws-sdk/client-cloudformation": "3.490.0",
    "@aws-sdk/client-cloudwatch-logs": "3.490.0",
    "@aws-sdk/client-dynamodb": "3.490.0",
    "@aws-sdk/client-ecr": "3.490.0",
    "@aws-sdk/client-ecs": "3.490.0",
    "@aws-sdk/client-eventbridge": "3.490.0",
    "@aws-sdk/client-iam": "3.490.0",
    "@aws-sdk/client-iot": "3.490.0",
    "@aws-sdk/client-iot-data-plane": "3.490.0",
    "@aws-sdk/client-lambda": "3.490.0",
    "@aws-sdk/client-ses": "3.490.0",
    "@aws-sdk/client-sqs": "3.490.0",
    "@aws-sdk/client-ssm": "3.490.0",
    "@aws-sdk/client-sts": "3.490.0",
    "@aws-sdk/client-timestream-write": "3.490.0",
    "@aws-sdk/util-dynamodb": "3.490.0",
    "@commitlint/config-conventional": "18.4.4",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "13.1.6",
    "@nordicsemiconductor/bdd-markdown": "7.0.15",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "17.0.0",
    "@nordicsemiconductor/firmware-ci-device-helpers": "14.0.201",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.102",
    "@types/aws-lambda": "8.10.131",
    "@types/chai": "4.3.11",
    "@types/chai-subset": "1.3.5",
    "@types/glob": "8.1.0",
    "@types/lodash-es": "4.17.12",
    "@types/semver": "7.5.6",
    "@types/ws": "8.5.10",
    "@types/yazl": "2.4.5",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "aws-cdk": "2.120.0",
    "aws-cdk-lib": "2.120.0",
    "cdk": "2.120.0",
    "chai": "5.0.0",
    "chai-subset": "1.6.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "11.1.0",
    "constructs": "10.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "50.0.1",
    "glob": "10.3.10",
    "globstar": "1.0.0",
    "husky": "8.0.3",
    "id128": "1.6.6",
    "jsonata": "2.0.3",
    "mqtt": "5.3.4",
    "p-retry": "6.2.0",
    "prettier": "3.1.1",
    "semver": "7.5.4",
    "tsmatchers": "5.0.2",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "ws": "8.16.0",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-lambda-powertools/metrics": "1.17.0",
    "@hello.nrfcloud.com/proto": "5.5.24",
    "@hello.nrfcloud.com/proto-lwm2m": "2.2.1",
    "@middy/core": "5.2.3",
    "@nordicsemiconductor/from-env": "3.0.0",
    "@nordicsemiconductor/random-words": "8.0.0",
    "@nordicsemiconductor/timestream-helpers": "6.0.1",
    "@sinclair/typebox": "0.31.28",
    "ajv": "8.12.0",
    "lodash-es": "4.17.21",
    "p-limit": "5.0.0",
    "table": "6.8.1"
  }
}
