{
  "name": "@nordicsemiconductor/muninn-backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for Muninn developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:e2e": "$(set -o pipefail && npx tsx ./feature-runner/run-features.ts | npx tsx ./feature-runner/console-reporter.ts)",
    "prepare": "husky install && check-node-version --package",
    "deploy": "cdk deploy",
    "synth": "cdk synth"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/Muninn-backend.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/Muninn-backend/issues"
  },
  "homepage": "https://github.com/bifravst/Muninn-backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "muninn",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "1.9.0",
    "@aws-sdk/client-apigatewaymanagementapi": "3.352.0",
    "@aws-sdk/client-cloudformation": "3.352.0",
    "@aws-sdk/client-dynamodb": "3.352.0",
    "@aws-sdk/client-ecr": "3.352.0",
    "@aws-sdk/client-ecs": "3.352.0",
    "@aws-sdk/client-eventbridge": "3.352.0",
    "@aws-sdk/client-iot": "3.352.0",
    "@aws-sdk/client-sqs": "3.352.0",
    "@aws-sdk/client-ssm": "3.352.0",
    "@aws-sdk/client-sts": "3.352.0",
    "@aws-sdk/client-timestream-write": "3.352.0",
    "@aws-sdk/util-dynamodb": "3.352.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.41",
    "@nordicsemiconductor/bdd-markdown": "3.6.17",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "15.0.9",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.64",
    "@swc/jest": "0.2.26",
    "@types/aws-lambda": "8.10.118",
    "@types/chai": "4.3.5",
    "@types/chai-subset": "1.3.3",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/ws": "8.5.5",
    "@types/yazl": "2.4.2",
    "aws-cdk": "2.83.1",
    "aws-cdk-lib": "2.83.1",
    "cdk": "2.83.1",
    "chai": "4.3.7",
    "chai-subset": "1.6.0",
    "chalk": "5.2.0",
    "check-node-version": "4.2.1",
    "commander": "10.0.1",
    "constructs": "10.2.52",
    "eslint-plugin-unicorn": "47.0.0",
    "glob": "10.2.7",
    "husky": "8.0.3",
    "id128": "1.6.6",
    "jest": "29.5.0",
    "jsonata": "2.0.3",
    "mqtt": "4.3.7",
    "p-retry": "5.1.2",
    "prettier": "2.8.8",
    "semver": "7.5.1",
    "tsx": "3.12.7",
    "typescript": "5.1.3",
    "ws": "8.13.0",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@bifravst/muninn-proto": "5.1.1",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@nordicsemiconductor/timestream-helpers": "5.0.0",
    "@sinclair/typebox": "0.28.15",
    "ajv": "8.12.0",
    "jsonwebtoken": "9.0.0",
    "p-limit": "4.0.0",
    "table": "6.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
