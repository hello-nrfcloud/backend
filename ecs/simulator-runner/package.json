  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/yargs": "^17.0.22",
    "cpy-cli": "^4.2.0",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.272.0",
    "@aws-sdk/util-dynamodb": "^3.272.0",
    "yargs": "^17.7.0"
  }
}
