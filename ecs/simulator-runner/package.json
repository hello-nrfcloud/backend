{
  "name": "simulator-runner",
  "version": "0.0.0-development",
  "description": "Run simulator devices using artillery",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node .",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf ./dist",
    "copy-files": "cpy './src/*.yml' ./dist && cpy './src/bin/**' ./dist/bin"
  },
